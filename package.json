  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "postinstall": "pnpm exec simple-git-hooks",
    "lint": "next lint --fix --max-warnings 0",
    "lint:check": "next lint --max-warnings 0",
    "package:install": "pnpm install",
    "package:reinstall": "rm -rf .next node_modules pnpm-lock.yaml && pnpm install",
    "package:update": "pnpm update --interactive",
    "prettier": "prettier --write --ignore-unknown .",
    "prettier:check": "prettier --check --ignore-unknown .",
    "serve": "next build && next start",
    "start": "next start",
    "test": "jest",
    "test:ci": "jest --ci",
    "test:watch": "jest --watch"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm exec lint-staged",
    "commit-msg": "pnpm exec commitlint --edit $1"
  },
  "prettier": {
    "plugins": [
      "prettier-plugin-packagejson",
      "prettier-plugin-tailwindcss"
